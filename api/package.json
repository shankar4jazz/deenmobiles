{
  "name": "deenmobiles-backend",
  "version": "1.0.0",
  "description": "Multi-branch mobile service management backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env PORT=5000 nodemon src/index.ts",
    "dev:cloud": "cross-env PORT=5000 nodemon src/index.ts",
    "dev:3000": "cross-env PORT=3000 nodemon src/index.ts",
    "build": "prisma generate && tsc",
    "postinstall": "prisma generate",
    "start": "cross-env PORT=5000 node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production PORT=5000 node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:reset": "prisma migrate reset",
    "import:sample-data": "ts-node src/scripts/importSampleData.ts",
    "test:api": "node test-api.js",
    "health": "curl http://localhost:5000/health",
    "health:detailed": "curl http://localhost:5000/health/detailed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "mobile",
    "service",
    "management",
    "repair"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@prisma/client": "^5.22.0",
    "@types/qrcode": "^1.5.6",
    "aws-sdk": "^2.1524.0",
    "axios": "^1.13.1",
    "bcrypt": "^5.1.1",
    "bwip-js": "^4.8.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "redis": "^4.6.12",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/pdfkit": "^0.17.3",
    "@types/uuid": "^10.0.0",
    "cross-env": "^10.1.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
